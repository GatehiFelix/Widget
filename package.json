{
  "name": "rag-server",
  "version": "1.0.0",
  "description": "Production-ready RAG system with Qdrant and multi-tenancy",
  "type": "module",
  "main": "server/main.js",
  "imports": {
    "#config/*": "./server/config/*",
    "#controllers/*": "./server/controllers/*",
    "#core/*": "./server/core/*",
    "#middleware/*": "./server/middleware/*",
    "#models/*": "./server/models/*",
    "#routes/*": "./server/routes/*",
    "#services/*": "./server/services/*",
    "#socket/*": "./server/socket/*",
    "#utils/*": "./server/utils/*"
  },
  "scripts": {
    "start": "node server/main.js",
    "server": " nodemon server/main.js",
    "client": " npm --prefix ./client run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rag",
    "langchain",
    "qdrant",
    "ollama",
    "vector-database",
    "llm",
    "multi-tenant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@langchain/community": "^0.3.20",
    "@langchain/core": "^0.3.24",
    "@langchain/google-genai": "^2.1.15",
    "@langchain/ollama": "^0.1.4",
    "@langchain/qdrant": "^0.1.3",
    "@qdrant/js-client-rest": "^1.12.0",
    "async-retry": "^1.3.3",
    "axios": "^1.13.2",
    "cheerio": "^1.2.0",
    "colors": "^1.4.0",
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "langchain": "^0.3.7",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.16.3",
    "node-cache": "^5.1.2",
    "nodemon": "^3.1.11",
    "p-queue": "^9.1.0",
    "p-timeout": "^7.0.1",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "concurrently": "^9.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
